<%
  list = Kit::Doc::Services::Modules.get_object_methods(
    object:                object,
    options:               options,
    include_class_methods: false,
    include_aliases:       true,
    verifier_runner:       ->(el) { run_verifier(el) },
  )
%>
<% if list.size > 0 %>

  <div class="summary summary-methods-instance">
    <h2 class='summary-heading'>
      <a href="#methods-instance" class='title'>Instance methods</a>
      <span class='list-count'><%= list.size %></span>
    </h2>

    <% list.each do |item| %>
      <%= yieldall item: item %>
    <% end %>
  </div>

<% end %>